{
  "name": "@teckmill/flex-theme-core",
  "version": "1.0.0",
  "description": "Core functionality for flex-theme",
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nom-nom-hub/FlexTheme.git",
    "directory": "packages/core"
  },
  "keywords": [
    "theme",
    "dark-mode",
    "light-mode",
    "typescript"
  ]
}
